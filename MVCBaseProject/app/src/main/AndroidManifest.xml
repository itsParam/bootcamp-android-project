<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="crats.mvcbaseproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.ProjectLists"
            android:label="Projects">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.PersonNamesListScreen"
            android:label="Example"
            android:parentActivityName=".view.ProjectLists"></activity>
        <activity android:name=".view.YashMainActivity"></activity>
        <activity android:name=".view.RetriveReview"></activity>
        <activity android:name=".view.ListView"></activity>
        <activity android:name=".view.FirstScreen"></activity>
        <activity android:name=".view.YashGiveReview"></activity>

        <meta-data
            android:name="com.google.android.gms.vision.DEPENDENCIES"
            android:value="barcode" />
        android:parentActivityName=".view.ProjectLists" />
        <activity android:name=".view.YoginMain" />
        <activity android:name=".view.PostListActivity" />
        <activity android:name=".view.AddPostActivity" />
        <activity android:name=".view.CreateAccountActivity" />
        <activity android:name=".view.SplashScreen" />
        <activity
            android:name=".view.MovieListActivity"
            android:parentActivityName=".view.ProjectLists" />
        <activity
            android:name=".view.MovieDetailActivity"
            android:parentActivityName=".view.MovieListActivity"></activity>
        <activity android:name=".view.ParamActivity"></activity>
    </application>

</manifest>